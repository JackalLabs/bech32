{
  "name": "@jackallabs/bech32",
  "version": "1.2.1",
  "description": "Bech32 encoding / decoding",
  "keywords": [
    "base32",
    "bech32",
    "bech32m",
    "bitcoin",
    "cosmos",
    "crypto",
    "crytography",
    "decode",
    "decoding",
    "encode",
    "encoding"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "packit": "npm run build && npm pack --pack-destination packs",
    "gen-docs": "npx typedoc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "contributors": [
    {
      "name": "Jackal Labs",
      "email": "devadmin@jackallabs.io",
      "url": "https://jackallabs.io"
    },
    {
      "name": "Erin Rivas",
      "email": "erin@jackallabs.io"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackalLabs/bech32.git"
  },
  "bugs": {
    "url": "https://github.com/JackalLabs/bech32/issues"
  },
  "homepage": "https://github.com/JackalLabs/bech32/#README",
  "devDependencies": {
    "@btmills/prettier": "3.0.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/browser": "^3.0.7",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^3.0.7"
  }
}
